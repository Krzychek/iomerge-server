<FindBugsFilter>
	<Match>
		<or>
			<Class name="pl.kbieron.iomerge.server.movementReader.MouseTrapReader"/>
			<Class name="pl.kbieron.iomerge.server.ui.SettingsWindow"/>
			<Class name="pl.kbieron.iomerge.server.ui.EdgeTrigger"/>
		</or>
		<Bug code="Se"/>
	</Match>

	<Match>
		<or>
			<and>
				<Class name="pl.kbieron.iomerge.server.appState.AppStateHolder"/>
				<Method name="onContextRefreshed"/>
			</and>
			<and>
				<Class name="pl.kbieron.iomerge.server.network.EventServer"/>
				<Method name="onAppStateChange"/>
			</and>
			<and>
				<Class name="pl.kbieron.iomerge.server.network.SingleClientHandler"/>
				<Method name="onAppStateChange"/>
			</and>
		</or>
		<Bug pattern="UPM_UNCALLED_PRIVATE_METHOD"/>
	</Match>

	<Match>
		<Class name="pl.kbieron.iomerge.server.network.SingleClientHandler"/>
		<Bug pattern="RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE"/>
	</Match>
</FindBugsFilter>